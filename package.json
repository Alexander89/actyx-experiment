{
  "name": "agv-experiment",
  "version": "0.1.0",
  "description": "Experiment, how to implement AGV use-cases with actyx",
  "scripts": {
    "node:agv:build": "tsc src/agv/index.ts --outDir build/agv --esModuleInterop --skipLibCheck",
    "node:agv:docker:build-aarch64": "npm run node:agv:build && docker buildx build --platform linux/arm64 -t agv-aarch64 -f src/agv/Dockerfile --load .",
    "node:agv:docker:build": "npm run node:agv:build && docker build -t agv -f src/agv/Dockerfile .",
    "node:agv:package": "ax apps package src/agv/ax-manifest.yml",
    "node:agv:start": "nodemon --watch src/agv --exec ts-node src/agv/index.ts",
    "node:dev:build": "tsc src/dev/index.ts --outDir build/dev --esModuleInterop --skipLibCheck",
    "node:dev:docker:build-aarch64": "npm run node:dev:build && docker buildx build --platform linux/arm64 -t dev-aarch64 -f src/dev/Dockerfile --load .",
    "node:dev:docker:build": "npm run node:dev:build && docker build -t dev -f src/dev/Dockerfile .",
    "node:dev:package": "ax apps package src/dev/ax-manifest.yml",
    "node:dev:start": "nodemon --watch src/dev --exec ts-node src/dev/index.ts",
    "node:mqttmockagv:build": "tsc src/mqttmockagv/index.ts --outDir build/mqttmockagv --esModuleInterop --skipLibCheck",
    "node:mqttmockagv:docker:build-aarch64": "npm run node:mqttmockagv:build && docker buildx build --platform linux/arm64 -t mqttmockagv-aarch64 -f src/mqttmockagv/Dockerfile --load .",
    "node:mqttmockagv:docker:build": "npm run node:mqttmockagv:build && docker build -t mqttmockagv -f src/mqttmockagv/Dockerfile .",
    "node:mqttmockagv:package": "ax apps package src/mqttmockagv/ax-manifest.yml",
    "node:mqttmockagv:start": "nodemon --watch src/mqttmockagv --exec ts-node src/mqttmockagv/index.ts",
    "ui:admin:build": "parcel build src/admin/index.html --out-dir src/admin/release --public-url ./",
    "ui:admin:package": "ax apps package src/admin/ax-manifest.yml",
    "ui:admin:start": "parcel src/admin/index.html --port 1234 --out-dir build/admin/debug",
    "ui:order-system:build": "parcel build src/order-system/index.html --out-dir src/order-system/release --public-url ./",
    "ui:order-system:package": "ax apps package src/order-system/ax-manifest.yml",
    "ui:order-system:start": "parcel src/order-system/index.html --port 1235 --out-dir build/order-system/debug",
    "ui:storage:build": "parcel build src/storage/index.html --out-dir src/storage/release --public-url ./",
    "ui:storage:package": "ax apps package src/storage/ax-manifest.yml",
    "ui:storage:start": "parcel src/storage/index.html --port 1236 --out-dir build/storage/debug",
    "ui:workstation:build": "parcel build src/workstation/index.html --out-dir src/workstation/release --public-url ./",
    "ui:workstation:package": "ax apps package src/workstation/ax-manifest.yml",
    "ui:workstation:start": "parcel src/workstation/index.html --port 1237 --out-dir build/workstation/debug",
    "fish:test": "TZ=UTC jest  --collectCoverageFrom=\"['!src/**/*.stories.tsx', './src/fish/**/*.{ts,tsx}', '!**/*.d.ts']\" --coverageDirectory=\"<rootDir>/coverage/fish\" ./src/fish",
    "fish:test:noCoverage": "TZ=UTC jest --coverage=false ./src/fish",
    "fish:test:watch": "TZ=UTC jest --coverage=false --watch ./src/fish",
    "node:event-emitter:start": "nodemon --watch src/event-emitter --exec ts-node src/event-emitter/index.ts",
    "node:event-emitter:build": "tsc src/event-emitter/index.ts --outDir build/event-emitter --esModuleInterop --skipLibCheck",
    "node:event-emitter:docker:build": "npm run node:event-emitter:build && docker build -t event-emitter -f src/event-emitter/Dockerfile .",
    "node:event-emitter:docker:build-aarch64": "npm run node:event-emitter:build && docker buildx build --platform linux/arm64 -t event-emitter-aarch64 -f src/event-emitter/Dockerfile --load .",
    "node:event-emitter:package": "ax apps package src/event-emitter/ax-manifest.yml",
    "ui:kanbanstation:start": "parcel src/kanbanstation/index.html --out-dir build/kanbanstation/debug",
    "ui:kanbanstation:build": "parcel build src/kanbanstation/index.html --out-dir src/kanbanstation/release --public-url ./",
    "ui:kanbanstation:package": "ax apps package src/kanbanstation/ax-manifest.yml",
    "ui:kanbanstation:test": "TZ=UTC jest  --collectCoverageFrom=\"['!src/**/*.stories.tsx', './src/kanbanstation/**/*.{ts,tsx}', '!**/*.d.ts']\" --coverageDirectory=\"<rootDir>/coverage/kanbanstation\" ./src/kanbanstation",
    "ui:kanbanstation:test:noCoverage": "TZ=UTC jest --coverage=false ./src/kanbanstation",
    "ui:kanbanstation:test:watch": "TZ=UTC jest --coverage=false --watch ./src/kanbanstation",
    "node:simpleagv:start": "nodemon --watch src/simpleagv --exec ts-node src/simpleagv/index.ts",
    "node:simpleagv:build": "tsc src/simpleagv/index.ts --outDir build/simpleagv --esModuleInterop --skipLibCheck",
    "node:simpleagv:docker:build": "npm run node:simpleagv:build && docker build -t simpleagv -f src/simpleagv/Dockerfile .",
    "node:simpleagv:docker:build-aarch64": "npm run node:simpleagv:build && docker buildx build --platform linux/arm64 -t simpleagv-aarch64 -f src/simpleagv/Dockerfile --load .",
    "node:simpleagv:package": "ax apps package src/simpleagv/ax-manifest.yml"
  },
  "keywords": [
    "actyx",
    "decentralized",
    "AGV"
  ],
  "author": "Alexander Halemba (Alexander89)",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "axp": {
    "axpVersion": "1.2.4",
    "pondVersion": 2
  },
  "dependencies": {
    "@actyx-contrib/react-pond": "^2.0.2",
    "@actyx-contrib/rx-pond": "^0.2.0",
    "@actyx/industrial-ui": "^1.0.5",
    "@actyx/os-sdk": "^0.3.1",
    "@actyx/pond": "^2.4.1",
    "aedes": "^0.44.2",
    "mqtt": "^4.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rxjs": "^6.6.3",
    "uuid": "^8.3.2"
  }
}
